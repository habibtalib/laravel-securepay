{
    "name": "habibtalib/laravel-securepay",
    "description": "Laravel package for SecurePay Malaysia payment gateway (FPX, DuitNow, Direct Debit)",
    "keywords": ["laravel", "securepay", "fpx", "duitnow", "payment", "malaysia"],
    "license": "MIT",
    "authors": [
        {
            "name": "Habib Talib",
            "email": "habib@pinc.my"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/http": "^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "HabibTalib\\SecurePay\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HabibTalib\\SecurePay\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "HabibTalib\\SecurePay\\SecurePayServiceProvider"
            ],
            "aliases": {
                "SecurePay": "HabibTalib\\SecurePay\\Facades\\SecurePay"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
